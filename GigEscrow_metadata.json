		"devdoc": {
			"details": "A robust freelance escrow system. Solves: Trust issues in remote work without the 20% platform fees. Features: 1. Safe Deposit: Client locks ETH. 2. Handshake: Freelancer must \"Accept\" to start. 3. Deadlines: Automated refunds if freelancer ghosts. 4. Arbitration: A neutral third party can resolve disputes.",
			"kind": "dev",
			"methods": {
				"acceptGig(uint256)": {
					"details": "Freelancer accepts the job. This locks the contract so Client can't just withdraw immediately."
				},
				"approveAndPay(uint256)": {
					"details": "Happy Path: Client is happy and releases funds to Freelancer."
				},
				"clawback(uint256)": {
					"details": "Safety Valve: If Freelancer ghosts (misses deadline and hasn't submitted), Client can pull funds back without Arbiter."
				},
				"createGig(address,address,string,uint256)": {
					"details": "Create a new Job. Client sends ETH here.",
					"params": {
						"_arbiter": "A neutral third party (e.g., a DAO address or mutual friend).",
						"_daysToComplete": "How many days the freelancer has to finish.",
						"_freelancer": "The wallet address of the worker.",
						"_title": "Job description."
					}
				},
				"raiseDispute(uint256)": {
					"details": "Sad Path: Either party can raise a dispute if things go wrong. Freezes the funds until Arbiter steps in."
				},
				"resolveDispute(uint256,uint256,uint256)": {
					"details": "Arbiter resolves dispute.",
					"params": {
						"_clientShare": "How much ETH goes back to Client (in Wei).",
						"_freelancerShare": "How much ETH goes to Freelancer (in Wei).",
						"_id": "The Gig ID."
					}
				},
				"submitWork(uint256)": {
					"details": "Freelancer marks work as done. Stops the deadline timer."
				}
			},
			"title": "BaseGig Escrow",
			"version": 1
